<section id="messages" class="messages right-col">
  <article id="messages-widget" class="messages-widget widget">
    <header class="title-container">
      <section class="user-name-container">
        <figure class="user-profile-picture" style="background-image: url('<%= static_path(@conn, "/images/profile-picture2.png") %>');" ></figure>
        <h1 class="user-name title">Kees is COOL</h1>
      </section>
    </header>
    <ul class="message-container">
      <li class="message">
        <div class="message-content">
          Heb jij de kaas gezien?
        </div>
        <time class="timestamp">Oct 19</time>
      </li>
      <li class="your-message message">
        <div class="message-content">
          Nope helaas niet haha
        </div>
        <time class="timestamp">Oct 20</time>
      </li>
      <li class="message">
        <div class="message-content">
          Okey haha
        </div>
        <time class="timestamp">Oct 20</time>
      </li>
      <li class="media-message message">
        <img class="media-item" src="<%= static_path(@conn, "/images/media/car.jpg") %>"alt="Media">
        <time class="timestamp">Oct 21</time>
      </li>
      <li class="your-message message">
        <div class="message-content">
          Hier jong heb je m terug xd
        </div>
