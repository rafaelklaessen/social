        <img class="media-item" src="<%= static_path(@conn, "/images/media/car.jpg") %>"alt="Media">
        <time class="timestamp">Oct 21</time>
      </li>
    </ul>
    <section class="message-form-container">
      <form class="message-form form" method="post">
        <input id="message-input" class="message-input input" type="text" placeholder="Send a message">
        <span class="media-btn grey-btn btn">
          <img class="icon" src="<%= static_path(@conn, "/images/icons/media-icon.png") %>" alt="Media icon">
        </span>
        <input id="send-message-btn" class="send-message-btn blue-btn btn" type="submit" value="Send">
      </form>
    </section>
  </article>
</section>